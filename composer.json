{
  "name": "resolventa/morphological-search",
  "description": "Morphological search for Russian, English and German languages ",
  "keywords": ["morphological search", "morphy", "php", "phpMorphy", "php-morphy", "phpmorphy"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Morphy\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.0",
    "makhov/phpmorphy": "dev-master",
    "ext-mbstring": "*",
    "psr/simple-cache": "1.0.x-dev"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "10.0.x-dev"
  }
}
